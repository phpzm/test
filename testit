#!/usr/bin/php -q
<?php

require_once 'vendor/autoload.php';

use Testit\App;

$app = new App([
    'root' => __DIR__,
    'environment' => env('ENVIRONMENT'),
    'domain' => env('DOMAIN'),
    'url' => env('URL'),
    'tests' => require __DIR__ . '/testit.tests.php',
    'defaults' => [
        'Headers' => [
            'Origin' => env('ORIGIN')
        ],
    ],
    'cookies' => []
]);

$app->run($argv);